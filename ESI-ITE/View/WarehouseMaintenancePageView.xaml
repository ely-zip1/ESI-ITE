<Page x:Class="ESI_ITE.View.WarehouseMaintenancePageView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ESI_ITE.View"
      mc:Ignorable="d" 
      d:DesignHeight="320" d:DesignWidth="640"
      Title="WarehouseMaintenancePageView">
    <Canvas Margin="10">
        <Image Height="100" Canvas.Left="10" Source="/ESI-ITE;component/Images/warehouse.png" Stretch="Fill" Canvas.Top="65" Width="100"/>
        <Label Content="Warehouse Maintenance" Height="35" Canvas.Left="240" Width="140" Canvas.Top="2"/>
        <Label Content="Warehouse Code" Height="23" Canvas.Left="150" Width="140" Canvas.Top="65"/>
        <ComboBox Height="20" Canvas.Left="150" Canvas.Top="93" Width="140"/>
        <Label Content="Warehouse Name" Height="23" Canvas.Left="150" Width="140" Canvas.Top="142"/>
        <TextBox Height="20" Canvas.Left="150" TextWrapping="Wrap" Canvas.Top="170" Width="460"/>
        <Button Content="Add" Height="30" Canvas.Left="65" Canvas.Top="260" Width="135"/>
        <Button Content="Delete" Height="30" Canvas.Left="240" Canvas.Top="260" Width="135"/>
        <Button Content="Exit" Height="30" Canvas.Left="420" Canvas.Top="260" Width="135"/>
    </Canvas>
</Page>
