<UserControl x:Class="ESI_ITE.View.InvoicingView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ESI_ITE.View"
             xmlns:vm="clr-namespace:ESI_ITE.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="720" d:DesignWidth="1540"
             Background="#FF181814">

    <UserControl.DataContext>
        <vm:InvoicingViewModel/>
    </UserControl.DataContext>
    <Frame Name="frameInvoice" Content="{Binding SelectedPage}" Padding="20 " NavigationUIVisibility="Hidden" Margin="5"
           HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Left" VerticalContentAlignment="Stretch"/>
</UserControl>
