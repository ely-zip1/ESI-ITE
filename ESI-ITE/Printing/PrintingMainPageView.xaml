<Page x:Class="ESI_ITE.Printing.PrintingMainPageView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ESI_ITE.Printing"
      xmlns:vm="clr-namespace:ESI_ITE.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PrintingMainPageView">
    <Page.DataContext>
        <vm:PrintingMainPageViewModel x:Name="viewModel"/>
    </Page.DataContext>
    <Grid>
        <DocumentViewer Document="{Binding DocSequence}"/>
    </Grid>
</Page>
